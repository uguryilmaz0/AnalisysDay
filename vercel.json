{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["fra1"],
  "crons": [
    {
      "path": "/api/cron/cleanup-analyses",
      "schedule": "0 4 * * *"
    },
    {
      "path": "/api/cron/cleanup-unverified-users",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/cleanup-old-analyses",
      "schedule": "0 5 * * 6"
    }
  ]
}
